<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_applicant" model="ir.sequence">
            <field name="name">Applicant Sequence</field>
            <field name="code">applicant.sequence</field>
            <field name="prefix">CAND/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_recruitment_order" model="ir.sequence">
            <field name="name">Recruitment Order</field>
            <field name="code">recruitment.order</field>
            <field name="prefix">RO/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>