<odoo>
	<record id="view_employee_tree_inherit" model="ir.ui.view">
        <field name="name">hr.employee.tree.views</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr_contract.view_employee_tree"/>
        <field name="arch" type="xml">
        	<field name="first_contract_date" position="attributes">
        		<attribute name="string">Hire Date</attribute>
        	</field>
        </field>
    </record>

    <record id="view_account_analytic_account_form_inherit" model="ir.ui.view">
        <field name="name">account.analytic.account.views</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="analytic.view_account_analytic_account_form"/>
        <field name="arch" type="xml">
            <field name="code" position="after">
                <field name="sds_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>
</odoo>
